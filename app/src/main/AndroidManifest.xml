<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aadpracticeproject">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_gads"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_gads_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".submit.SubmitActivity" android:parentActivityName=".leaderboard.MainActivity"
            android:theme="@style/AppTheme.Submit"/>
        <activity android:name=".leaderboard.MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>